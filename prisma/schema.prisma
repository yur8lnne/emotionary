datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int     @id @default(autoincrement())
  userId    String  @unique
  password  String
  username  String?
  email     String? @unique
  phone     String?
  createdAt DateTime @default(now())
}
