generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  
}

model User {
  id        Int      @id @default(autoincrement())
  userId    String   @unique
  password  String
  name      String?
  email     String?  @unique
  phone     String?  @unique
  createdAt DateTime @default(now())
}
